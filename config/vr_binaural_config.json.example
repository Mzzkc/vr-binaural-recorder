{
  "audio": {
    "bufferSize": 128,
    "sampleRate": 48000,
    "inputDevice": "default",
    "outputDevice": "default",
    "api": "wasapi",
    "channels": {
      "input": 1,
      "output": 2
    },
    "format": {
      "bitDepth": 32,
      "float": true
    }
  },
  "hrtf": {
    "dataPath": "./hrtf_data/",
    "dataset": "kemar_mit",
    "filterLength": 512,
    "interpolation": true,
    "convolutionMethod": "overlap_add",
    "nearFieldCompensation": true
  },
  "spatial": {
    "maxDistance": 10.0,
    "referenceDistance": 1.0,
    "rolloffFactor": 1.0,
    "distanceModel": "inverse",
    "dopplerEffect": false,
    "roomSize": "medium",
    "enableReverb": false
  },
  "ui": {
    "enableManualPositioning": true,
    "showDebugInfo": false,
    "theme": "dark",
    "autoHide": true,
    "autoHideDelay": 5000
  },
  "performance": {
    "threadPriority": "high",
    "simdLevel": "auto",
    "useMemoryPool": true,
    "ringBufferSize": 4096
  },
  "logging": {
    "level": "info",
    "console": true,
    "file": true,
    "path": "./logs/"
  },
  "debug": {
    "enableMetrics": false,
    "verboseAudio": false,
    "testMode": false,
    "skipHRTFLoading": false
  }
}